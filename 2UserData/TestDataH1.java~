public class TestDataH1
{
	public static void main(String[] args)
	{
		// declaring integers
		int happiness = 18;
	
		// declaring decimal numbers
		double doofus = 2.8141;
		
		// declaring characters
		char fish = '*';
		
		// declaring boolean
		boolean salt = true;
		
		// declaring String
		String congress = "Not debate!";
		
		final int quartInGallon = 4;
		/* We can't do this to a final constant
		quartInGallon = 5;
		*/
		
		System.out.println("The value of doofus is " + doofus);
		System.out.println("The value of happiness is " + happiness);
		System.out.println("The value of fish is " + fish);
		System.out.println("Today is my birthday! " + salt);
		System.out.println("What is congress? " + congress);
	}
}